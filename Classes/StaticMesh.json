{
	"name": "StaticMesh",
	"description": "A StaticMesh entity represents a Mesh which can be spawned in the world, can't move and is more optimized for using in decorating the world.",
	"authority": "both",
	"network_distribution": "disabled",
	"inheritance": [
		"Entity",
		"Actor",
		"Paintable"
	],
	"constructors": [
		{
			"name": "Default Constructor",
			"parameters": [
				{
					"type": "Vector",
					"name": "location"
				},
				{
					"type": "Rotator",
					"name": "rotation"
				},
				{
					"type": "StaticMeshPath",
					"name": "static_mesh_asset"
				},
				{
					"type": "CollisionType",
					"name": "collision_type",
					"default": "CollisionType.Auto"
				}
			]
		}
	],
	"functions": [
		{
			"name": "SetMesh",
			"description": "Changes the mesh in runtime",
			"parameters": [
				{
					"type": "StaticMeshPath",
					"name": "static_mesh_asset",
					"description": "asset"
				}
			],
			"authority": "authority",
			"efficiency": "slow"
		},
		{
			"name": "GetMesh",
			"description": "Gets the Asset path mesh used",
			"return": [
				{
					"type": "StaticMeshPath",
					"description": "asset path",
					"name": "static_mesh_asset"
				}
			],
			"authority": "both",
			"efficiency": "fast"
		},
		{
			"name": "IsFromLevel",
			"description": "Gets if this StaticMesh is from the Level",
			"return": [
				{
					"type": "boolean",
					"description": "if this StaticMesh is from the level",
					"name": "is_from_level"
				}
			],
			"authority": "client",
			"efficiency": "fast"
		}
	]
}
